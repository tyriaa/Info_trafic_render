{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "globals": {
    "fetch": "readonly",
    "console": "readonly",
    "document": "readonly",
    "window": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "error"
  }
}
